<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">



<xs:element name="lista_de_personas">
    
    <xs:complexType>
        <xs:sequence>
        <xs:element name="persona" minOccurs="1" maxOccurs="2" >
            <xs:complexType>
      <xs:sequence>
        <xs:element name="nombre" type="xs:string"/>
        <xs:element name="fechanacimiento" type="xs:string" minOccurs="1" />
        <xs:element name="telefono" type="xs:string" minOccurs="1" maxOccurs="2"/>
        
        <xs:element name="sexo" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="numeroseguridadsocial" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="nacionalidad" type="xs:string" use="required"/>
    </xs:complexType>

    </xs:element> 
    </xs:sequence>
    </xs:complexType>
    </xs:element>

    </xs:schema>